Layout from \"@/layout/BaseLayout\";\r\nimport axios from \"../../plugins/axios\";\r\n\r\nexport default {\r\n  components: {\r\n    BaseLayout,\r\n  },\r\n\r\n  mounted() {\r\n    //getting user data\r\n    this.carregaLista();\r\n  },\r\n  data() {\r\n    return {\r\n      carros: [],      \r\n    };\r\n  },\r\n  methods: {\r\n\r\n    carregaLista() {\r\n      axios.get(\"http://127.0.0.1:8000/carro\").then((response) => {\r\n        console.log(response);\r\n        this.carros = response.data;\r\n      });\r\n    },\r\n\r\n    //add new user\r\n    create_user() {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/create\", this.form)\r\n        .then((resp) => {\r\n          console.log(resp);\r\n          this.loadlist();\r\n          //reset form\r\n          this.form.name = \"\";\r\n          this.form.email = \"\";\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"]}]}