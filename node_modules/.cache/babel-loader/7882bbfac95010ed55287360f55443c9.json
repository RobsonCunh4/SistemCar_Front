te","key","_toDisplayString","nome","telefone","href","_ctx","_hoisted_13","$options","_hoisted_14","_hoisted_15","_createCommentVNode","_hoisted_19","required","$event","cpf","_hoisted_25"],"sourceRoot":"","sources":["C:\\xampp\\htdocs\\projects\\Start_3\\front\\src\\views\\Cliente\\Cliente.vue"],"sourcesContent":["<template>\r\n  <BaseLayout>\r\n    <template v-slot:slot-pages>\r\n      <div class=\"content-pages bg-light\">\r\n        <div class=\"container mb-2\">\r\n          <div class=\"row\">\r\n            <div\r\n              v-if=\"response.message\"\r\n              :class=\"`alert alert-${response.tipo}`\"\r\n              role=\"alert\"\r\n            >\r\n              {{ response.message }}\r\n            </div>\r\n            <div class=\"col-md-8\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-success\"\r\n                data-toggle=\"modal\"\r\n                data-target=\"#Modalcliente\"\r\n                @click=\"showModal = true\"\r\n              >\r\n                Adicionar cliente\r\n              </button>\r\n            </div>\r\n            <div class=\"col-md-4\">\r\n              <input\r\n                type=\"text\"\r\n                placeholder=\"Pesquisar\"\r\n                class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"container mb-5\">\r\n          <div class=\"row\">\r\n            <div class=\"col-md-12 mb-5\">\r\n              <table class=\"table table-bordered table-striped table-hover\">\r\n                <thead>\r\n                  <tr>\r\n                    <th scope=\"col-sm-1\">Código</th>\r\n                    <th scope=\"col-sm-6\">Nome</th>\r\n                    <th scope=\"col-sm-4\">Telefone</th>\r\n                    <th scope=\"col-sm-1\">Ações</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"cliente in clientes\" :key=\"cliente.id\">\r\n                    <td class=\"text-center\" scope=\"row\"><strong>{{ cliente.id }}</strong></td>\r\n                    <td>{{ cliente.nome }}</td>\r\n                    <td>{{ cliente.telefone }}</td>\r\n                    <td>\r\n                      <div class=\"btn-group\">\r\n                       <a\r\n                        href=\"#\"\r\n                        class=\"btn btn-primary\"\r\n                        @click.stop.prevent=\"getDetalhe(cliente)\"\r\n                        >Detalhes</a\r\n                      >\r\n                      <a\r\n                        href=\"#\"\r\n                        class=\"btn btn-warning\"\r\n                        @click.stop.prevent=\"getCliente(cliente)\"\r\n                        >Editar</a\r\n                      >\r\n                      <a\r\n                        href=\"#\"\r\n                        class=\"btn btn-danger\"\r\n                        @click.stop.prevent=\"removerCliente(cliente.id)\"\r\n                        >Remover</a\r\n                      ></div>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Modal -->\r\n      <div\r\n        v-if=\"showModal\"\r\n        class=\"modal fade\"\r\n        id=\"Modalcliente\"\r\n        tabindex=\"-1\"\r\n        role=\"dialog\"\r\n        aria-labelledby=\"exampleModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog\" role=\"document\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h5 class=\"modal-title\" id=\"Modalcliente\">Cadastro de cliente</h5>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-group mb-3\">\r\n                <input\r\n                  required=\"\"\r\n                  type=\"text\"\r\n                  placeholder=\"Nome\"\r\n                  class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n                  v-model=\"form.nome\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group mb-3\">\r\n                <input\r\n                  required=\"\"\r\n                  type=\"text\"\r\n                  placeholder=\"CPF\"\r\n                  class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n                  v-model=\"form.cpf\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group mb-3\">\r\n                <input\r\n                  required=\"\"\r\n                  type=\"number\"\r\n                  placeholder=\"Telefone\"\r\n                  class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n                  v-model=\"form.telefone\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div class=\"modal-footer\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-secondary\"\r\n                data-dismiss=\"modal\"\r\n              >\r\n                Cancelar\r\n              </button>                      \r\n              <button type=\"button\" class=\"btn btn-primary\" @click.stop.prevent=\"CadastrarCliente()\">Salvar</button>\r\n              <button type=\"button\" class=\"btn btn-primary\" @click=\"showModal = false\">teste</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </template>\r\n  </BaseLayout>\r\n</template>\r\n\r\n<script>\r\nimport BaseLayout from \"@/layout/BaseLayout\";\r\nimport axios from \"../../plugins/axios\";\r\n\r\nexport default {\r\n  components: {\r\n    BaseLayout,\r\n  },\r\n\r\n  mounted() {\r\n    this.carregaLista();\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      clientes: [],\r\n      form: {\r\n        id: \"\",\r\n        nome: \"\",\r\n        cpf: \"\",\r\n        telefone: \"\",\r\n      },\r\n      response: {\r\n        tipo: \"\",\r\n        message: \"\",\r\n      },\r\n      showModal : false      \r\n    };\r\n  },\r\n\r\n  methods: {\r\n    carregaLista() {\r\n      axios.get(\"/cliente\").then((response) => {\r\n        console.log(response);\r\n        this.clientes = response.data;\r\n      });\r\n    },\r\n\r\n    getCliente(cliente) {\r\n      console.log(\"getCliente: \" + cliente);\r\n    },\r\n\r\n    async CadastrarCliente() {\r\n      axios\r\n        .post(\"/cliente/\", this.form)\r\n        .then(() => {\r\n          this.response.tipo = \"success\";\r\n          this.response.message = \"Cadastro realizado com sucesso!\";\r\n          this.ResetFormulario();\r\n          this.carregaLista();          \r\n        })\r\n        .catch(() => {\r\n          this.response.tipo = \"danger\";\r\n          this.response.message = \"Informações inválidas\";\r\n        });\r\n    },\r\n\r\n    ResetFormulario() {\r\n      (this.form.id = \"\"), (this.form.nome = \"\"), (this.form.telefone = \"\");\r\n    },\r\n\r\n   async removerCliente(id) {\r\n      axios.delete(\"/cliente/\" + id).then(() => {\r\n        this.response.tipo = \"success\";\r\n        this.response.message = \"Cadastro removido com sucesso!\";          \r\n        this.carregaLista();\r\n      });\r\n    },\r\n    \r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"]}]}