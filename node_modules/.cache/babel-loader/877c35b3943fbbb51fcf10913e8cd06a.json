               <th scope=\"col\">Placa</th>\r\n                    <th scope=\"col\">Ações</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"post in posts\" :key=\"post.id\">\r\n                    <td scope=\"row\">{{ post.id }}</td>\r\n                    <td>{{ post.name }}</td>\r\n                    <td>{{ post.email }}</td>\r\n                    <td>\r\n                      <a\r\n                        href=\"#\"\r\n                        class=\"btn btn-warning\"\r\n                        @click=\"get_user(post.id, post.name, post.email)\"\r\n                        >Edit</a\r\n                      >\r\n                      <a\r\n                        href=\"#\"\r\n                        class=\"btn btn-danger\"\r\n                        @click=\"delete_user(post.id)\"\r\n                        >Delete</a\r\n                      >\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </template>\r\n  </BaseLayout>\r\n</template>\r\n\r\n<script>\r\nimport BaseLayout from \"@/layout/BaseLayout\";\r\nimport axios from \"../../plugins/axios\";\r\n\r\nexport default {\r\n  components: {\r\n    BaseLayout,\r\n  },\r\n\r\n  mounted() {\r\n    //getting user data\r\n    this.carregaLista();\r\n  },\r\n  data() {\r\n    return {\r\n      posts: [],\r\n      edituser: false,\r\n      adduser: true,\r\n      form: {\r\n        name: \"\",\r\n        email: \"\",\r\n      },\r\n      upd_user: {\r\n        id: null,\r\n        name: \"\",\r\n        email: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    //getting all users details\r\n    carregaLista() {\r\n      axios.get(\"http://127.0.0.1:8000/carro\").then((resp) => {\r\n        console.log(resp);\r\n        this.posts = resp.data;\r\n      });\r\n    },\r\n\r\n    //add new user\r\n    create_user() {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/create\", this.form)\r\n        .then((resp) => {\r\n          console.log(resp);\r\n          this.loadlist();\r\n          //reset form\r\n          this.form.name = \"\";\r\n          this.form.email = \"\";\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"]}]}