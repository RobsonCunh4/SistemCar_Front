\n              v-if=\"response.message\"\r\n              :class=\"`alert alert-${response.tipo} alert-dismissible`\"\r\n              role=\"alert\"\r\n            >\r\n              {{ response.message }}\r\n              <div\r\n                class=\"progress active\"\r\n                role=\"progressbar\"\r\n                style=\"height: 5px\"\r\n              >\r\n                <div\r\n                  :class=\"`progress-bar bg-${response.tipo}`\"\r\n                  :style=\"`width: ${progresso}%; opacity: 0.5`\"\r\n                ></div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-md-7\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-success\"\r\n                data-toggle=\"modal\"\r\n                data-target=\"#Modalcliente\"\r\n                @click=\"ResetFormulario()\"\r\n              >\r\n                Adicionar cliente\r\n              </button>\r\n            </div>\r\n            <div class=\"col-md-5\">\r\n              <div class=\"input-group mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  placeholder=\"Pesquisar por nome\"\r\n                  class=\"form-control border-0 shadow-sm px-4\"\r\n                  v-model=\"filtro\"                  \r\n                  aria-describedby=\"btnPesquisar\"\r\n                  v-on:keyup.enter=\"filtrarClientes()\"\r\n                />\r\n                <div class=\"input-group-append\">\r\n                  <button\r\n                    class=\"btn btn-secondary\"\r\n                    type=\"button\"\r\n                    id=\"btnPesquisar\"\r\n                    @click=\"filtrarClientes()\"\r\n                  >\r\n                    Pesquisar\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"container mb-5\">\r\n          <div class=\"row\">\r\n            <div class=\"col-md-12 mb-5 table-wrapper\">\r\n              <table class=\"table table-bordered table-striped table-hover\">\r\n                <thead>\r\n                  <tr>\r\n                    <th scope=\"col-sm-1\">Código</th>\r\n                    <th scope=\"col-sm-6\">Nome</th>\r\n                    <th scope=\"col-sm-4\">Telefone</th>\r\n                    <th scope=\"col-sm-1\" class=\"gridAcoes\">Ações</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"cliente in clientes\" :key=\"cliente.id\">\r\n                    <td class=\"text-center\" scope=\"row\">\r\n                      <strong>{{ cliente.id }}</strong>\r\n                    </td>\r\n                    <td>{{ cliente.nome }}</td>\r\n                    <td>{{ cliente.telefone }}</td>\r\n                    <td class=\"gridAcoes\">\r\n                      <div class=\"btn-group\">\r\n                        <a\r\n                          href=\"#\"\r\n                          class=\"btn btn-primary\"\r\n                          data-toggle=\"modal\"\r\n                          data-target=\"#Modalcliente\"\r\n                          @click=\"detalharCliente(cliente)\"\r\n                          >Detalhes</a\r\n                        >\r\n                        <a\r\n                          type=\"button\"\r\n                          class=\"btn btn-warning\"\r\n                          data-toggle=\"modal\"\r\n                          data-target=\"#Modalcliente\"\r\n                          @click=\"PreencherForm(cliente)\"\r\n                          >Atualizar</a\r\n                        >\r\n                        <button\r\n                          type=\"button\"\r\n                          class=\"btn btn-danger\"\r\n                          @click.stop.prevent=\"removerCliente(cliente.id)\"\r\n                        >\r\n                          Remover\r\n                        </button>\r\n                      </div>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"Modalcliente\"\r\n        tabindex=\"-1\"\r\n        role=\"dialog\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog\" role=\"document\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h5 class=\"modal-title\" id=\"Modalcliente\">{{ tituloModal }}</h5>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-group mb-3\">\r\n                <input\r\n                  required=\"\"\r\n                  type=\"text\"\r\n                  placeholder=\"Nome\"\r\n                  name=\"nome\"\r\n                  class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n                  v-model=\"form.nome\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group mb-3\">\r\n                <input\r\n                  required=\"\"\r\n                  type=\"text\"\r\n                  placeholder=\"CPF\"\r\n                  class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n                  v-model.trim=\"form.cpf\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  name=\"numbers\"\r\n                  placeholder=\"Telefone\"\r\n                  class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n                  v-model.trim=\"form.telefone\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div class=\"modal-footer\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-secondary\"\r\n                data-dismiss=\"modal\"\r\n              >\r\n                Fechar\r\n              </button>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-primary\"\r\n                v-show=\"btnSalvar\"\r\n                @click.stop.prevent=\"salvarCadastro()\"\r\n              >\r\n                {{ descBotao }}\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </template>\r\n  </BaseLayout>\r\n</template>\r\n\r\n<script>\r\nimport BaseLayout from \"@/layout/BaseLayout\";\r\nimport axios from \"../../plugins/axios\";\r\n\r\nexport default {\r\n  components: {\r\n    BaseLayout,\r\n  },\r\n\r\n  mounted() {\r\n    this.carregarLista();\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      clientes: [],\r\n      clientes_data: [],\r\n      form: {\r\n        id: null,\r\n        nome: \"\",\r\n        cpf: \"\",\r\n        telefone: \"\",\r\n      },\r\n      response: {\r\n        tipo: \"\",\r\n        message: \"\",\r\n      },\r\n      cadastrar: true,\r\n      tituloModal: \"Cadastrar cliente\",\r\n      descBotao: \"Salvar\",\r\n      btnSalvar: true,\r\n      campos: true,\r\n      progresso: 100,\r\n      filtro: \"\",\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    carregarLista() {\r\n      axios.get(\"/cliente\").then((response) => {\r\n        this.clientes = response.data;\r\n        this.clientes_data = response.data;\r\n      });\r\n    },\r\n\r\n    PreencherForm(cliente) {\r\n      this.ResetFormulario();\r\n      this.tituloModal = \"Atualizar cliente\";\r\n      this.cadastrar = false;\r\n      this.btnSalvar = true;\r\n      this.form.id = cliente.id;\r\n      this.form.nome = cliente.nome;\r\n      this.form.telefone = cliente.telefone;\r\n      this.form.cpf = cliente.cpf;\r\n    },\r\n\r\n    async editarCliente() {\r\n      axios\r\n        .put(\"/cliente/\" + this.form.id, this.form)\r\n        .then(() => {\r\n          this.carregarLista();\r\n          this.ResetFormulario();\r\n          this.fecharModal();\r\n          this.notificacao(\"warning\", \"Cadastro atualizado com sucesso!\");\r\n        })\r\n        .catch(() => {\r\n          this.notificacao(\"danger\", \"Erro ao realizar a edição do cadastro.\");\r\n          this.fecharModal();\r\n        });\r\n    },\r\n\r\n    detalharCliente(cliente) {\r\n      this.PreencherForm(cliente);\r\n      this.btnSalvar = false;\r\n      this.tituloModal = \"Detalhes cliente\";\r\n    },\r\n\r\n    async CadastrarCliente() {\r\n      axios\r\n        .post(\"/cliente/\", this.form)\r\n        .then(() => {\r\n          this.ResetFormulario();\r\n          this.carregarLista();\r\n          this.fecharModal();\r\n          this.notificacao(\"success\", \"Cadastro realizado com sucesso!\");\r\n        })\r\n        .catch(() => {\r\n          this.notificacao(\"danger\", \"Informações inválidas ao cadastrar.\");\r\n          this.fecharModal();\r\n        });\r\n    },\r\n\r\n    async removerCliente(id) {\r\n      axios\r\n        .delete(\"/cliente/\" + id)\r\n        .then((resp) => {\r\n          this.notificacao(\"success\", \"Cadastro removido com sucesso!\");\r\n          this.carregarLista();\r\n          console.log(resp);\r\n        })\r\n        .catch((resp) => {\r\n          console.log(resp);\r\n          this.notificacao(\"danger\", \"Erro ao realizar a remoção do cadastro.\");\r\n        });\r\n    },\r\n\r\n    ResetFormulario() {\r\n      (this.form.id = null),\r\n        (this.form.nome = \"\"),\r\n        (this.form.cpf = \"\"),\r\n        (this.form.telefone = \"\"),\r\n        (this.cadastrar = true);\r\n      this.tituloModal = \"Cadastrar cliente\";\r\n      this.btnSalvar = true;\r\n    },\r\n\r\n    fecharModal() {\r\n      document.getElementById(\"Modalcliente\").click();\r\n    },\r\n\r\n    salvarCadastro() {\r\n      if (this.cadastrar == true) {\r\n        this.CadastrarCliente();\r\n      } else {\r\n        this.editarCliente();\r\n      }\r\n    },\r\n\r\n    notificacao(tipo, msg) {\r\n      this.response.tipo = tipo;\r\n      this.response.message = msg;\r\n\r\n      const tempo = setInterval(() => {\r\n        this.progresso--;\r\n        if (this.progresso <= 0) {\r\n          this.response.message = null;\r\n          clearInterval(tempo);\r\n          this.progresso = 100;\r\n        }\r\n      }, 50);\r\n    },\r\n\r\n    filtrarClientes() {\r\n      this.resetData();\r\n      if (this.filtro) {\r\n        this.clientes = this.clientes.filter((cliente) => {\r\n          return cliente.nome.toLowerCase().includes(this.filtro.toLowerCase());\r\n        });\r\n      }\r\n    },\r\n\r\n    resetData() {\r\n      this.clientes = this.clientes_data;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.gridAcoes {\r\n  width: 15px;\r\n}\r\n\r\n.table-wrapper {\r\n  max-height: 600px;\r\n  overflow: auto;\r\n  display: inline-block;\r\n}\r\n\r\n.alert {\r\n  position: relative;\r\n}\r\n\r\n.progress {\r\n  position: absolute;\r\n  width: 100%;\r\n  bottom: 0px;\r\n  left: 0px;\r\n}\r\n</style>\r\n"]}]}