\n  <div class=\"container-fluid\">\r\n    <div class=\"row no-gutter\">\r\n      <div class=\"col-md-6 d-none d-md-flex bg-image\"></div>\r\n\r\n      <div class=\"col-md-6 bg-light\">\r\n        <div class=\"login d-flex align-items-center py-5\">\r\n          <div class=\"container\">\r\n            <div class=\"row\">\r\n              <div class=\"col-lg-10 col-xl-7 mx-auto\">\r\n                <h3 class=\"display-4\">SistemCars</h3>\r\n                <p class=\"text-secondary mb-4\">\r\n                  Realize o login para ter acesso ao sistema!\r\n                </p>\r\n\r\n                <div\r\n                  v-if=\"response.message\"\r\n                  class=\"alert alert-danger\"\r\n                  role=\"alert\"\r\n                >\r\n                  {{ response.message }}\r\n                </div>\r\n                <form class=\"mt-4\">\r\n                  <div class=\"form-group mb-3\">\r\n                    <input\r\n                      type=\"email\"\r\n                      placeholder=\"Email\"\r\n                      required=\"\"\r\n                      autofocus=\"\"\r\n                      class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n                      v-model=\"email\"\r\n                    />\r\n                  </div>\r\n                  <div class=\"form-group mb-3\">\r\n                    <input\r\n                      required=\"\"\r\n                      id=\"inputPassword\"\r\n                      type=\"password\"\r\n                      placeholder=\"Password\"\r\n                      class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n                      v-model=\"senha\"\r\n                    />\r\n                  </div>\r\n                  <div class=\"form-group mb-3\">\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"\r\n                        btn btn-primary\r\n                        rounded-pill\r\n                        shadow-sm\r\n                        btn-block\r\n                        w-100\r\n                      \"\r\n                      @click.stop.prevent=\"login()\"\r\n                    >\r\n                      Entrar\r\n                    </button>\r\n                  </div>\r\n                </form>\r\n                <div class=\"d-flex justify-content-end mt-4\">\r\n                  <p>\r\n                    Novo usuário?\r\n                    <router-link :to=\"{ name: 'registro' }\"\r\n                      ><u>Realize o cadastro.</u></router-link\r\n                    >\r\n                  </p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"../../plugins/axios\";\r\nimport Cookie from \"@/service/cookie\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      senha: \"\",\r\n      response: {        \r\n        message: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      const payload = {\r\n        email: this.email,\r\n        password: this.senha,\r\n      };\r\n\r\n      this.ResetResponse();\r\n\r\n      axios\r\n        .post(\"/auth/login\", payload)\r\n        .then((response) => {\r\n          const token = `${response.data.token_type} ${response.data.token}`;\r\n          Cookie.setToken(token);\r\n          this.$router.push({name: 'dashboard'});\r\n        })\r\n        .catch(() => {          \r\n          this.response.message = \"E-mail e/ou senha inválidos\";\r\n        });\r\n    },\r\n\r\n    ResetResponse() {\r\n      this.response.color = \"\";\r\n      this.response.message = \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.login,\r\n.image {\r\n  min-height: 100vh;\r\n}\r\n.bg-image {\r\n  background-image: url(\"~@/assets/imagens/login.jpg\");\r\n  background-size: cover;\r\n  background-position: center center;\r\n}\r\n</style>\r\n\r\n\r\n\r\n"]}]}