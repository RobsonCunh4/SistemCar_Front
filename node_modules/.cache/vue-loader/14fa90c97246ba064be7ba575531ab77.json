fluid\">\r\n    <div class=\"row no-gutter\">\r\n      <div class=\"col-md-6 d-none d-md-flex bg-image\"></div>\r\n\r\n      <div class=\"col-md-6 bg-light\">\r\n        <div class=\"login d-flex align-items-center py-5\">\r\n          <div class=\"container\">\r\n            <div class=\"row\">\r\n              <div class=\"col-lg-10 col-xl-7 mx-auto\">\r\n                <h3 class=\"display-4\">SistemCars</h3>\r\n                <p class=\"text-primary mb-4\">\r\n                  Preencha as informações para realizar o seu cadastro.\r\n                </p>\r\n                <div\r\n                  v-if=\"response.message\"\r\n                  :class=\"`alert alert-${response.tipo}`\"\r\n                  role=\"alert\"\r\n                >\r\n                  {{ response.message }}\r\n                </div>\r\n                <form>\r\n                  <div class=\"form-group mb-3\">\r\n                    <input\r\n                      v-model=\"nome\"\r\n                      type=\"text\"\r\n                      placeholder=\"Nome completo\"\r\n                      required=\"\"\r\n                      autofocus=\"\"\r\n                      class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n                    />\r\n                  </div>\r\n                  <div class=\"form-group mb-3\">\r\n                    <input\r\n                      v-model=\"email\"\r\n                      type=\"email\"\r\n                      placeholder=\"Email\"\r\n                      required=\"\"\r\n                      class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n                    />\r\n                  </div>\r\n                  <div class=\"form-group mb-3\">\r\n                    <input\r\n                      v-model=\"senha\"\r\n                      type=\"password\"\r\n                      placeholder=\"Password\"\r\n                      required=\"\"\r\n                      class=\"form-control rounded-pill border-0 shadow-sm px-4\"\r\n                    />\r\n                  </div>\r\n                  <div class=\"form-group mb-3\">\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"\r\n                        btn btn-primary\r\n                        rounded-pill\r\n                        shadow-sm\r\n                        btn-block\r\n                        w-100\r\n                      \"\r\n                      @click.stop.prevent=\"registro()\"\r\n                    >\r\n                      Registrar\r\n                    </button>\r\n                  </div>\r\n                  <div class=\"d-flex justify-content-end mt-4\">\r\n                    <p>\r\n                      Já tem um usuário?\r\n                      <a href=\"#\" class=\"font-italic text-primary\">\r\n                        <u>Realize o login.</u>\r\n                      </a>\r\n                    </p>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"../../plugins/axios\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n\r\n  data() {\r\n    return {\r\n      nome: \"\",\r\n      email: \"\",\r\n      senha: \"\",\r\n      response: {\r\n        tipo: \"\",\r\n        message: \"\",\r\n      },\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async registro() {\r\n      const payload = {\r\n        name: this.nome,\r\n        email: this.email,\r\n        password: this.senha,\r\n      };\r\n\r\n      axios\r\n        .post(\"/auth/registro\", payload)\r\n        .then(() => {\r\n          this.response.tipo = \"success\";\r\n          this.response.message = \"Cadastro realizado com sucesso!\";\r\n          this.ResetFormulario();\r\n        })\r\n        .catch(() => {\r\n          this.response.tipo = \"danger\";\r\n          this.response.message = \"Informações inválidas\";\r\n        });\r\n    },\r\n\r\n    ResetFormulario() {\r\n      this.nome = '',\r\n      this.email = '',\r\n      this.senha = ''\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.login,\r\n.image {\r\n  min-height: 100vh;\r\n}\r\n.bg-image {\r\n  background-size: cover;\r\n  background-position: center center;\r\n}\r\n</style>\r\n\r\n\r\n\r\n"]}]}