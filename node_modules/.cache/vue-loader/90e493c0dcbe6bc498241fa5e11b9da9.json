d (event) {\n      if (this.state.customSelection) return\n\n      let source = event.target || event.srcElement\n      if (source.tagName.toLowerCase() === 'td') {\n        if (this.isSelected) {\n          this.store.deselectRow(this.row)\n        } else {\n          this.store.selectRow(this.row)\n        }\n      }\n    }\n  }\n}\n</script>\n"]}]}